{
  "telemetry.enableCrashReporter": false,
  "telemetry.enableTelemetry": false,
  "grunt.autoDetect": "off",
  "gulp.autoDetect": "off",
  "jake.autoDetect": "off",
  "search.followSymlinks": false,
  "search.exclude": {
    "node_modules": true,
    ".history": true,
    ".nyc_output": true,
    "dist/": true,
    "doc": true,
    "coverage": true
  },
  "files.autoSave": "afterDelay",
  "files.autoSaveDelay": 1500,
  "files.exclude": {
    "**/.git": true,
    "**/.DS_Store": true,
    ".history": true,
    ".nyc_output": true,
    "node_modules": true
  },
  "files.watcherExclude": {
    "**/.git/objects/**": true,
    "**/.git/subtree-cache/**": true,
    ".history": true,
    ".nyc_output": true,
    "node_modules": true,
    "dist": true,
    "doc": true,
    "coverage": true
  },
  "cSpell.ignoreWords": [
    "preversion",
    "postversion",
    "newversion",
    "mtfe",
    "mtjfsc",
    "gitlab",
    "cnpm",
    "zhengxs",
    "foxmail",
    "docdash",
    "buble",
    "filesize",
    "esnext",
    "recurse",
    "monospace",
    "standardjs",
    "rollupjs",
    "repo",
    "miniprogram",
    "wechat",
    "cookiejar"
  ],
  "cSpell.ignorePaths": [
    "**/package-lock.json",
    "**/node_modules/**",
    "**/vscode-extension/**",
    "**/.git/**",
    ".vscode",
    ".history",
    ".nyc_output",
    "coverage",
    "dist",
    "doc",
    "CHANGELOG.md"
  ],
  "eslint.autoFixOnSave": true
}
